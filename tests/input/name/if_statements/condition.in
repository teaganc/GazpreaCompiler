procedure main() returns integer {
  integer x = 0;
  character new_line = '\n';

  /* print out numbers
    who are divisible
    by 2, 3, or 5
  */

  loop while x < 100 {
    if(x % 2 == 0) {
      x -> std_output;
      new_line -> std_output;
    } else if (x % 3 == 0) {
      x -> std_output;
      new_line -> std_output;
    } else if (x % 5 == 0) {
      x -> std_output;
      new_line -> std_output;
    }

    x = x + 1;
  }

  return 0;
}