    real small_positive = 1.18e-38;
    real integer_as_float = 1e1;
    real zero = 0e10;
    real no_exponent = 1.33;
    character new_line = '\n';

    /* testing the output values*/

    large_negative -> std_output;
    new_line -> std_output;

    small_negative -> std_output;
    new_line -> std_output;

    large_positive -> std_output;
    new_line -> std_output;

    small_positive -> std_output;
